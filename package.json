    "prettier": "^2.8.4",
    "sass": "^1.59.2",
    "sass-loader": "^10.2.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^29.0.0"
  }
}
