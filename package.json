{
  "name": "henkebyte-website",
  "version": "1.0.0",
  "private": true,
  "description": "A showcase website for different open source web projects including an iOS-Calculator and Minesweeper.",
  "homepage": "https://henkebyte.com",
  "bugs": {
    "url": "https://github.com/manuelhenke/henkebyte/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelhenke/henkebyte.git"
  },
  "license": "MIT",
  "author": {
    "name": "Manuel Henke",
    "email": "contact@henkebyte.com",
    "url": "https://henkebyte.com/"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && nuxt generate",
    "build-analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:js": "eslint --ext \".js,.jsx,.mjs,.cjs,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install",
    "test": "jest --passWithNoTests",
    "generate-preview-screenshot": "node ./generate-preview-image.cjs"
  },
  "dependencies": {
    "@egjs/flicking-plugins": "^4.7.0",
    "@egjs/vue-flicking": "^4.10.6",
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "chart.js": "^4.2.1",
    "contentful": "^9.3.3",
    "dexie": "^3.2.3",
    "fireworks-js": "^2.10.2",
    "ios-calculator-for-web": "^1.2.14",
    "lodash-es": "^4.17.21",
    "masonry-layout": "^4.2.2",
    "minesweeper-for-web": "^3.0.4",
    "nuxt": "^2.15.8",
    "vue-clickaway": "^2.2.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@types/bootstrap": "^5.2.6",
    "@types/lodash-es": "^4.17.6",
    "@types/masonry-layout": "^4.2.5",
    "@types/vue-clickaway": "^2.2.0",
    "async": "^3.2.4",
    "babel-jest": "^29.4.3",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-babel": "^0.2.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^46.0.0",
